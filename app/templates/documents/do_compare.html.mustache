<div id="wiki-wrapper" class="compare">
<div id="head">
  <h1>History for <strong>{{page_title}}</strong></h1>
  
  <ul class="actions">
  <li class="minibutton"><a href="{{pages_list_path}}"
       class="action-view-page">Page List</a></li>
    
    <li class="minibutton"><a href="{{history_document_path}}"
       class="action-page-history">Back to Page History</a></li>
  </ul>
</div>

{{#message}}
<p>{{message}}</p>
{{/message}}

<div id="compare-content">
  
  {{#show_revert}}
  <ul class="actions">
    <li class="minibutton">
      {{{revert_document_form_tag}}}
      <a href="#" class="gollum-revert-button">Revert Changes</a>
      </form>
    </li>
  </ul>
  {{/show_revert}}
  
  <div class="data highlight">
    <table cellpadding="0" cellspacing="0">
      {{#lines}}
        <tr>
          <td class="line_numbers">{{ldln}}</td>
          <td class="line_numbers">{{rdln}}</td>
          <td>
            <pre><div class="{{class}}">{{line}}</div></pre>
          </td>
        </tr>
      {{/lines}}
    </table>
  </div>
</div>
<div id="footer">
  <ul class="actions">
    {{#show_revert}}
    <li class="minibutton">
      <a href="#" class="gollum-revert-button">Revert Changes</a>
    </li>
    {{/show_revert}}
    <li class="minibutton"><a href="#">Back to Top</a></li>
  </ul>
</div>
</div>
</div>
